	private static String regexMobile = '((090|080|070)[-| ]?\\d{4}[-| ]?\\d{4})';
	private static List<RegTag> regLst;
	private static String regexSpical;
	private PhoneNumberUtils(){
		// 获取XML的值
		regLst = new List<RegTag>();
		regexSpical = '';
		try {
			List<Dom.XmlNode> topNodeXmlList = CommXmlUtils.getAllTopXmlNode('PhoneConfig',new List<String>{'',''});
			for(Dom.XmlNode d : topNodeXmlList){
				String nodeName = d.getName();
				// 判断输入的值与XML的值进行匹配
				if (nodeName == 'SpicalReg') {
					regexSpical = d.getText();
				} else if (nodeName == 'RegTag'){
					RegTag r = new RegTag(d);
					regLst.add(r);
				}
			}
		} catch (CommException ex) {}
	}
	
	// 2016/07/18 日本電話分割機能
	public static string splitNumber(String phoneNumber,String splitStr) {
		//携带电话
		String phone = CommUtils.nullToBlank(phoneNumber);
		if(!CommUtils.isBlank(phone))
			phone = phone.replaceAll('[^0-9]', '');
        //10桁 || 11桁　以外の場合、解析しません
        if (phone.length() != 10 && phone.length() != 11) return phone;
        
        Pattern isnumbersPattern = Pattern.Compile(regexMobile);
		Matcher numberMatch = isnumbersPattern.matcher(phone);
		if (phone.length() == 11) { 
			if (numberMatch.find()) {
				return phone.substring(0,3) + '-' + phone.substring(3,7) + '-' + phone.substring(7,11);
			} else return phoneNumber;
		} else {
			// 固定電話解析処理を行う
			return instance.splitNumberByString(phone,splitStr);
		}
	}
    private string splitNumberByString(String phoneNumber,String splitStr){
    	if(CommUtils.isBlank(splitStr)) splitStr = '-';
		Pattern isnumbersPattern = Pattern.Compile(regexSpical);
		Matcher numberMatch = isnumbersPattern.matcher(phoneNumber);
		String lastNumber = '';
		String findNumber = '';
		// 如果匹配对SpicalReg值，不匹配RegTag中值，则得到map的值，输出结果 ，否则输出结果形式3-3-4形式
		if(numberMatch.find()) { 
			findNumber = numberMatch.group();
			if (spicalMap01.containsKey(findNumber))
				lastNumber = spicalMap01.get(findNumber);
			else if (spicalMap02.containsKey(findNumber)) 
				lastNumber = spicalMap02.get(findNumber);
			phoneNumber = phoneNumber.replaceFirst(findNumber,'');
			
			return lastNumber + splitStr + phoneNumber;
		} else {
			for (RegTag r : regLst) {
				Pattern commPattern = Pattern.Compile(r.regStr);
				numberMatch = commPattern.matcher(phoneNumber);
				if (numberMatch.find()) {
					findNumber = numberMatch.group();
					return r.getNumber(findNumber,phoneNumber,splitStr);
				}
			}
		}
		return phoneNumber.subString(0,3) + splitStr + phoneNumber.subString(3,6) + splitStr + phoneNumber.subString(6);
	}
	class RegTag{
		String regStr;
		String template;
		List<Integer> splitIntLst;
		public RegTag(Dom.XmlNode d){
			regStr = d.getText();
			splitIntLst = new List<Integer>();
			template = d.getAttribute('template',null);
			if(!CommUtils.isBlank(template)){
				for( String numberSize : template.split('-')){
					if (CommUtils.isBlank(numberSize)) continue;
					splitIntLst.add(CommUtils.nullToIntZero(numberSize));
				}
			}
		}
		public String getNumber(String firstNumber,String phoneNumber,String splitString){
			phoneNumber = phoneNumber.replaceFirst(firstNumber,'');
			String secondNumber = phoneNumber.subString(0,splitIntLst[1]);
			phoneNumber = phoneNumber.replaceFirst(secondNumber,'');
			return firstNumber + splitString + secondNumber + splitString + phoneNumber;
		}
	}
