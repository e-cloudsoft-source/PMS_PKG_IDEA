               // 異常
               if(event.type == 'exception') { 
                   console.log(event.message);
               } else {
               		//&& guuid == result.guuid
                   if(result && result.tdInf.length > 0 ){
                       var rsLst = result.tdInf;
                       RowInfs.push(rsLst);
