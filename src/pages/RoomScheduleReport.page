	for (var i = 0; i < orgBgLst.length ; i++){
		$j("td." + orgBgLst[i]).removeClass(orgBgLst[i]);
	}
	$j("td." + getOrgClass()).removeClass(getOrgClass());
	orgBgLst.length = 0;
	curBackClass = "";
}
