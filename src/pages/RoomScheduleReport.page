function updatePlanItemMapFun(rowIndex){
	var _row = getRowInfoFun(rowIndex); // 获取当前PLAN行数据集
	if(_row.actionType != ACTTYPE_PLAN) return;
	var planKey = getPlanKeyFun(rowIndex), // 获取当前PLAN的KEY
