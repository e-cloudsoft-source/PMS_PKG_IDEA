function getObjX(a) {
    var left = 0;       
    while(a){
        left += (a.offsetLeft - a.scrollLeft + a.clientLeft);
        a = a.offsetParent;
    }
    return left;
}
function getObjY(a) {
    var top = 0;        
    while(a){
        top += (a.offsetTop - a.scrollTop + a.clientTop);
        a = a.offsetParent;
    }
    return top;
}
