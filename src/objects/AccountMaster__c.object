				var records = result.getArray(&quot;records&quot;);
				if(records.length &lt;= 0){
				     alert(&quot;{!$Label.ps__msg_012_0473}&quot;);
				} else if(records.length &gt; 50){
                                      alert(&quot;{!$Label.ps__msg_012_0472}&quot;);
				} else {
					var ids = [];
					for(var i = 0; i &lt; records.length; i++){
						ids.push(records[i].Id);
					}
var url = &quot;{!URLFOR(&apos;/apex/&apos; &amp; $Setup.CommDefine__c.AppNS__c &amp; &apos;DinnerPDF&apos;)}&quot;;
url += &quot;?ids=&quot; + ids.join(&quot;&amp;ids=&quot;);
					window.open(url);					
				}
			}</url>
