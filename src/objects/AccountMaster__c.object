        <url>{!REQUIRESCRIPT(&apos;/soap/ajax/35.0/connection.js&apos;)}

			var selectedContactIds = {!GETRECORDIDS($ObjectType.AccountMaster__c)
			};

			if(selectedContactIds[0] == null) {
				alert(&quot;{!$Label.ps__msg_012_0473}&quot;);
			} else {
				var sql = &quot;select id from ps__AccountMaster__c where ps__CookCount__c &gt; 0 and id in(&apos;&quot; + selectedContactIds.join(&quot;&apos;,&apos;&quot;) + &quot;&apos;)&quot;;
