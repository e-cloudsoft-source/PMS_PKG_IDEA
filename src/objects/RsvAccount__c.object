        <requireRowSelection>true</requireRowSelection>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/35.0/connection.js&quot;)}
var lockFlg = {!$User.AccountLockFlg__c};
if(!lockFlg) alert(&apos;権限がありません。&apos;);
else{
var records = {!GETRECORDIDS($ObjectType.RsvAccount__c)};
if (records.length &gt; 0) {
var rsvs = [];
records.forEach(function(rsvId){
var rsv = new sforce.SObject(&quot;ps__RsvAccount__c&quot;);
rsv.Id = rsvId;
rsv.ps__IsLockFlg__c = true;
rsvs.push(rsv);
});
var result = sforce.connection.update(rsvs);
window.location.reload(true);
} else {
alert(&apos;一件以上選択してください。&apos;);
}
}</url>
