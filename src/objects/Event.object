    <validationRules>
        <fullName>RoomSettingContactCheck</fullName>
        <active>true</active>
        <description>予約自動作成のお客様チェック</description>
        <errorConditionFormula>IF ( ISBLANK( RoomRef__c) , False, IF ( ISBLANK( WhoId), True, false) )</errorConditionFormula>
        <errorMessage>お部屋を設定されている場合、名前「お客様(取引先責任者)」を設定してください。</errorMessage>
    </validationRules>
    <webLinks>
        <fullName>MulitiAccounSelectBtn</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>取引先選択</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>var eventWhatId = &quot;{!Event.What}&quot;; 
var sObjectType = eventWhatId.substring(0,3); 
if(sObjectType == &apos;001&apos; || sObjectType == &apos;&apos;){ 
var url = &quot;{!URLFOR(&apos;/apex/&apos;+$Setup.CommDefine__c.AppNS__c+&apos;AccountMulitiSelector&apos;)}&quot;;
if (url.includes(&quot;?&quot;)) {
 url += &apos;&amp;id={!Event.Id}&apos;;
 url += &apos;&amp;retURL=&apos; + window.location.href;
} else url += &apos;?id={!Event.Id}&apos;;
window.open(url,&apos;&apos;,&apos;width=700,height=550,scrollbars=yes&apos;);
}else { 
alert(&apos;関連先は取引先以外のとき、「取引先選択」ボタンが使えません&apos;); 
}</url>
    </webLinks>
