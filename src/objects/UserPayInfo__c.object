        <fullName>CardNoInputCheck</fullName>
        <active>true</active>
        <description>決済方法が「カード」の場合、カード番号を入力必須</description>
        <errorConditionFormula>AND( ISPICKVAL( PayType__c , &quot;カード&quot;), LEN( CardNo__c  )= 0  )</errorConditionFormula>
        <errorDisplayField>CardNo__c</errorDisplayField>
        <errorMessage>カード番号を入力して下さい</errorMessage>
    </validationRules>
