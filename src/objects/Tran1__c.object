        <externalId>false</externalId>
        <formula>if ( Field7__r.nextitemcode__c &gt;= 0 &amp;&amp; Field7__r.nextitemcode__c &lt;= 3000 &amp;&amp;  Field1__r.TeaTimeFlag__c = 0 &amp;&amp; (  Categorya__c =&quot;A1_料理関連&quot; || Categorya__c =&quot;B1_ドリンク関連&quot; ), 1, if ( Field7__r.nextitemcode__c &gt;= 0 &amp;&amp; Field7__r.nextitemcode__c &lt;= 3000 &amp;&amp; ( Categorya__c =&quot;B1_ドリンク関連&quot; ||  Categorya__c =&quot;A1_料理関連&quot; ), 2, if ( Field7__r.nextitemcode__c &gt;= 3100 &amp;&amp; Field7__r.nextitemcode__c &lt;= 4000 &amp;&amp; ( Field7__r.OP__r.OPcode1__c = 32 || Field7__r.OP__r.OPcode1__c = 33 ), 3, if ( Field7__r.nextitemcode__c &gt;= 7100 &amp;&amp; Field7__r.nextitemcode__c &lt;= 8600 &amp;&amp; ( Categorya__c =&quot;A1_料理関連&quot; || Categorya__c =&quot;B1_ドリンク関連&quot; ), 4, if ( Field7__r.OP__r.OPcode1__c = 30, 5 , 0)))))</formula>
